{
  "name": "packweb",
  "description": "Tools for managing projects with large numbers of npm modules",
  "keywords": [
    "appium",
    "packages",
    "npm",
    "tools"
  ],
  "version": "1.4.0",
  "author": "appium",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/packweb.git"
  },
  "bugs": {
    "url": "https://github.com/appium/packweb/issues"
  },
  "engines": [
    "node"
  ],
  "main": "./build/index.js",
  "bin": {
    "packweb": "./bin/packweb.js"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib",
    "build/index.js",
    "build/lib",
    "bin"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@octokit/rest": "^16.0.1",
    "appium-support": "^2.17.0",
    "argparse": "^1.0.10",
    "asyncbox": "^2.0.2",
    "bluebird": "^3.5.1",
    "cli-color": "^1.0.0",
    "fancy-log": "^1.3.2",
    "lodash": "^4.17.10",
    "npm": "^6.1.0",
    "request": "^2.55.0",
    "request-promise": "^4.2.2",
    "semver": "^6.0.0",
    "source-map-support": "^0.5.5"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install",
    "prepare": "gulp prepublish",
    "test": "gulp once",
    "watch": "gulp watch",
    "lint": "gulp eslint",
    "lint:fix": "gulp eslint --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit-test": "REPORTER=dot gulp once",
    "build": "gulp prepublish"
  },
  "devDependencies": {
    "ajv": "^6.5.3",
    "appium-gulp-plugins": "^4.0.0",
